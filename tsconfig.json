{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": false,
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "outDir": "out"
  },
  "lib": [
    "es2015",
    "dom"
  ],
  "typeRoots": [
    "./node_modules/@types"
  ],
  "exclude": [
    "out",
    "node_modules",
    "dist",
    "dev",
    "reports"
  ],
  "version": "2.0.6"
}