{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "es2015",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "moduleResolution": "node",
    "types": [
      "hammerjs",
      "jasmine",
      "node"
    ],
    "lib": ["es2015", "dom"]
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "exclude": [
    "out",
    "node_modules",
    "dist",
    "dev",
    "reports"
  ],
  "version": "2.0.6"
}